  "version": "0.0.3",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/ethereum-optimism/actions.git",
    "directory": "packages/demo/frontend"
  },
  "scripts": {
    "build": "NODE_OPTIONS='--max-old-space-size=4096' tsc -b && vite build",
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "dev": "vite",
    "preview": "vite preview",
    "lint": "eslint \"**/*.{ts,tsx}\" && pnpm prettier --check \"**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"**/*.{ts,tsx}\" --fix --quiet && pnpm prettier \"**/*.{ts,tsx}\" --write --loglevel=warn",
    "test": "vitest --run",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@dynamic-labs/ethereum": "4.31.4",
    "@dynamic-labs/sdk-react-core": "4.31.4",
    "@eth-optimism/actions-sdk": "workspace:*",
    "@eth-optimism/actions-service": "workspace:*",
    "@eth-optimism/viem": "^0.4.13",
    "@privy-io/react-auth": "^2.24.0",
    "@tanstack/react-query": "^5.90.9",
    "@turnkey/react-wallet-kit": "^1.1.2",
    "buffer": "^6.0.3",
    "eventemitter3": "^5.0.1",
    "highlight.js": "^11.11.1",
    "mixpanel-browser": "^2.72.0",
    "react": "^18",
    "react-dom": "^18",
    "react-router": "7",
    "react-router-dom": "7",
    "react-scrolly-telling": "^0.2.2",
