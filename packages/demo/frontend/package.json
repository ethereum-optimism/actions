    "@testing-library/user-event": "^14.5.1",
    "@types/react": "^18",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^9.22.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "globals": "^15.0.0",
    "jsdom": "^23.0.1",
    "postcss": "^8.4.31",
    "tailwindcss": "^4.1.15",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.0.0",
    "vite": "^5.0.0",
    "vitest": "^1.6.1"
